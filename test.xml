<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="AbhiBus Test Suite" parallel="classes" thread-count="6" data-provider-thread-count="20">
    <test name="Bus Search - Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.abhibus.BusSearchTest"/>
        </classes>
    </test>
    <test name="Bus Search - Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.abhibus.BusSearchTest"/>
        </classes>
    </test>
</suite>